@import '~vuetify/src/stylus/main'

.playfair-font {
  font-family: 'Playfair Display', serif!important;
  &-bold {
    font-weight bold;
    &-italic {
      font-style: italic;
    }
  }
}

.extra-letter-spacing {
  letter-spacing 1.5px!important;
  &-intro {
    letter-spacing 3px!important;
  }
}

